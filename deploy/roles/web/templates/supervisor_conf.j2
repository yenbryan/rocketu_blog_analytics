[group:{{ project_name }}]
programs=gunicorn_{{ project_name }}

[program:gunicorn_{{ project_name }}]
command={{ virtualenv_path }}/bin/newrelic-admin run-program {{ virtualenv_path }}/bin/gunicorn -c gunicorn.conf.py -p gunicorn.pid wsgi:application --pythonpath {{ project_path }}/rocketu_blog_analytics
directory={{ project_path }}
user=ubuntu
autostart=true
autorestart=true
redirect_stderr=true
environment=NEW_RELIC_CONFIG_FILE="{{ project_path }}/deploy/newrelic.ini"